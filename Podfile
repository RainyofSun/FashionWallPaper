platform :ios, '8.0'
use_frameworks!

def all_Pods
    pod 'YYKit'
    pod 'MBProgressHUD'
    pod 'MJExtension'
    pod 'Bytedance-UnionAD'
    pod 'MJRefresh'
    pod 'AFNetworking', '~> 3.1.0', :subspecs => ['Reachability', 'Serialization', 'Security', 'NSURLSession']
end

target 'FashionWallPaper' do
   all_Pods
end
